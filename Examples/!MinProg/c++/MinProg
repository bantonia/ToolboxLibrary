#include "MinProg.h"

void MinProg::toolboxEventHandler(int event) {
  event = event;
}

void MinProg::toolboxUserEvent() {
  ToolboxEventHeader *tboxHdr = (ToolboxEventHeader *) this->getPollBlock();
  switch(tboxHdr->event_code) {
    case Quit: {
        this->closeDown();
      }
      break;
    default:
      break;
  }
}
 
void MinProg::toolboxObjectAutoCreated(int object_class) {
  object_class = object_class;
}
