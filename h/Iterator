#include "Vector.h"

template<class Vector<T> >
class Iterator {
  public:
    Iterator(Vector<T> v) {
      this.v = v;
      index = 0;
    };
    ~Iterator() {};
    T next() {
      if (v.capacity() > 0 && v.capacity() < index) {
        return v.get(++index);
      }
      return NULL;
    };
  private:
    Vector<T> v;
    int index;
};
