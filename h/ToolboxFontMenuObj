    int getFont();
    int getFont(char *buffer, int bufSize);
    ToolboxFontMenuObj& setObjectId(ObjectId objectId);
    ToolboxFontMenuObj& setFlags(unsigned int flags);
    ToolboxFontMenuObj& clearFlags();
};

