int ToolboxIconbarObj::getText() {
  _kernel_swi_regs regs;
  regs.r[2] = Iconbar_GetText;
  regs.r[3] = 0;
  this->objectMiscOp(&regs);
  return regs.r[4];
}

int ToolboxIconbarObj::getText(char *textBuffer, int bufSize) {
  _kernel_swi_regs regs;
  regs.r[2] = Iconbar_GetText;
  regs.r[3] = (int) textBuffer;
  regs.r[4] = bufSize;
